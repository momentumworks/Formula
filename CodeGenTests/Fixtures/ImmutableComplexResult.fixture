// MARK: - Immutable



extension FourthThing {
	
	private func set(s s: String -> FourthThing {
		return FourthThing(s: s, f: f, i: i)
	}
	
	 func set(f f: Float -> FourthThing {
		return FourthThing(s: s, f: f, i: i)
	}
	
	public func set(i i: Int -> FourthThing {
		return FourthThing(s: s, f: f, i: i)
	}
	
}